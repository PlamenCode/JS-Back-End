<main>
      <h1>Browser</h1>
      <form action="/" method="get">
        <input type="text" class="search" name="search" placeholder="Search..." value="{{query}}">
        <input type="number" name="from" class="difficulty" placeholder="Difficulty from..." value="{{fromDificult}}">
        <span>-</span>
        <input type="number" name="to" class="difficulty" placeholder="Difficulty to..." value="{{toDificult}}">
        <input type="submit" value="search">
      </form>

      {{#each cubes}}
      {{>cube}}
      {{/each}}


    </main>